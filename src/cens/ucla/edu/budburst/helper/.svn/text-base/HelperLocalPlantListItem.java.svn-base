package cens.ucla.edu.budburst.helper;

public class HelperLocalPlantListItem {
	public HelperLocalPlantListItem(int Category, String CommonName, String ScienceName, String County,
			String State, String UsdaUrl, String CopyRight, int ImageID) {
		this.Category = Category;
		this.CommonName = CommonName;
		this.ScienceName = ScienceName;
		this.County = County;
		this.UsdaUrl = UsdaUrl;
		this.CopyRight = CopyRight;
		this.ImageID = ImageID;
	}
	
	public int Category;
	public int ImageID;
	public String CommonName;
	public String ScienceName;
	public String County;
	public String State;
	public String UsdaUrl;
	public String CopyRight;		
}
